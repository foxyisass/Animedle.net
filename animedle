        };
 
        let selectedCharacter = null;
        let guessedCharacters = new Set();
 
        function chooseRandomCharacter() {
            const keys = Object.keys(characters);
            selectedCharacter = characters[keys[Math.floor(Math.random() * keys.length)]];
        }
 
        function populateCharacterList() {
            const characterList = document.getElementById('characterList');
            for (const characterName in characters) {
                const option = document.createElement('option');
                option.value = characterName;
                characterList.appendChild(option);
            }
        }
 
        // ...
 
function getClass(value, correctValue, property) {
    if (property === 'haki') {
        const hasCommonHaki = value.some(haki => correctValue.includes(haki));
        return hasCommonHaki ? 'correct' : 'incorrect'; // Grün, wenn übereinstimmung, Rot, wenn nicht
    } else if (['herkunft', 'arc'].includes(property)) {
        const isMatch = value.toLowerCase() === correctValue.toLowerCase();
        return isMatch ? 'correct' : 'incorrect'; // Grün, wenn stimmt, Rot, wenn nicht
    }
 
    return value === correctValue ? 'correct' : 'incorrect'; // Für die anderen Eigenschaften
}
 
function getDirection(value, correctValue, property) {
    if (typeof value === 'number' && typeof correctValue === 'number') {
        if (value > correctValue) {
            return '↓'; // Pfeil nach unten, wenn Wert größer ist
        } else if (value < correctValue) {
            return '↑'; // Pfeil nach oben, wenn Wert kleiner ist
        }
    }
    return ''; // Kein Pfeil, wenn nicht nummerisch
}
 
function checkCharacter() {

    const input = document.getElementById('name').value.trim();
    if (!input) {
        alert("Bitte gib einen Namen ein!");
        return;
    }
    if (guessedCharacters.has(input)) {
        alert("Diesen Charakter hast du bereits geraten!");
        return;
    }
 
    const guessedCharacter = characters[input];
    if (!guessedCharacter) {
        alert("Charakter nicht gefunden!");
        return;
    }
 
    guessedCharacters.add(input);
 
    const row = document.createElement('tr');
    row.innerHTML = `
        <td>
            <img src="${guessedCharacter.bild}" alt="${input}" 
             style="width: 90px; height: 90px; border-radius: 5px; object-fit: cover;">
        </td>
        <td>${input}</td>
        <td class="${getClass(guessedCharacter.geschlecht, selectedCharacter.geschlecht)}">${guessedCharacter.geschlecht}</td>
        <td class="${getClass(guessedCharacter.zugehörigkeit, selectedCharacter.zugehörigkeit)}">${guessedCharacter.zugehörigkeit}</td>
        <td class="${getClass(guessedCharacter.teufelsfrucht, selectedCharacter.teufelsfrucht)}">${guessedCharacter.teufelsfrucht}</td>
        <td class="${getClass(guessedCharacter.haki, selectedCharacter.haki, 'haki')}">${guessedCharacter.haki.join(' ')}</td>
        <td class="${getClass(guessedCharacter.kopfgeld.replace(' Mio.', '').replace(' Mrd.', '').replace(' ', '').replace('.', ''), selectedCharacter.kopfgeld.replace(' Mio.', '').replace(' Mrd.', '').replace(' ', '').replace('.', ''), 'kopfgeld')}" title="${guessedCharacter.kopfgeld}">${guessedCharacter.kopfgeld} ${getDirection(parseFloat(guessedCharacter.kopfgeld.replace(' Mio.', '').replace(' Mrd.', '').replace(' ', '').replace('.', '')), parseFloat(selectedCharacter.kopfgeld.replace(' Mio.', '').replace(' Mrd.', '').replace(' ', '').replace('.', '')), 'kopfgeld')}</td>
        <td class="${getClass(guessedCharacter.größe, selectedCharacter.größe, 'größe')}" title="${guessedCharacter.größe} cm">${guessedCharacter.größe} cm ${getDirection(guessedCharacter.größe, selectedCharacter.größe, 'größe')}</td>
        <td class="${getClass(guessedCharacter.herkunft, selectedCharacter.herkunft)}">${guessedCharacter.herkunft}</td>
        <td class="${getClass(guessedCharacter.arc, selectedCharacter.arc)}" title="${guessedCharacter.arc}">${guessedCharacter.arc} ${getDirection(guessedCharacter.arc, selectedCharacter.arc, 'arc')}</td>
    `;
 
    // Füge die neue Zeile am Anfang des Table-Bodys hinzu
    document.querySelector('#result-table tbody').insertBefore(row, document.querySelector('#result-table tbody').firstChild);
}
        window.onload = function () {
            chooseRandomCharacter();
            populateCharacterList();
        };
    </script>
</body>
 
</html>
